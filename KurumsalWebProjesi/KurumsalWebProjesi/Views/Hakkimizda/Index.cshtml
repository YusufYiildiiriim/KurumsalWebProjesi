@model IEnumerable<KurumsalWebProjesi.Models.Model.Hakkimizda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@*<a href="/Hakkimizda/Add">Yeni Ekle</a>*@

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Aciklama)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.Raw(item.Aciklama)
            </td>
            <td>
                @Html.ActionLink("Güncelleme", "Edit", new { id = item.HakiimizdaId })
            </td>
        </tr>
    }

</table>
